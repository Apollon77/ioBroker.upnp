{
    "common": {
        "name":                     "upnp",
        "version":                  "0.3.5",
        "title":                    "upnp adapter",
        "desc":                     {
 			"en": "ioBroker upnp Adapter",
			"de": "ioBroker upnp Adapter",
			"ru": "ioBroker upnp драйвер как образец"
	},
        "platform":                 "Javascript/Node.js",
        "mode":                     "daemon",
        "icon":                     "upnp-discovery.png",
		"extIcon":          "https://raw.githubusercontent.com/Jey-Cee/ioBroker.upnp/master/admin/upnp-discovery.png",
		"readme":           "https://github.com/Jey-Cee/ioBroker.upnp/blob/master/README.md",
		"loglevel":         "info",
        "type":                     "hardware"
    },
    "native": {
    },
    "instanceObjects": [
      {
        "_id":  "enableAutoDiscover",
        "type": "state",
        "common": {
          "role":  "state",
          "name":  "Auto discovery activated",
          "desc":  "If Auto discovery on or off",
          "type":  "boolean",
          "read":  true,
          "write": true
        },
        "native": {}
      },{
        "_id":  "enableAutoSubscription",
        "type": "state",
        "common": {
          "role":  "state",
          "name":  "Auto subscription activated",
          "desc":  "If Auto subscription on or off",
          "type":  "boolean",
          "read":  true,
          "write": true
        },
        "native": {}
      }
  ]
}
